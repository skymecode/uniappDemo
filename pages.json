{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
	
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
		    "path" : "pages/friends/friends",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "动态",
		        "enablePullDownRefresh": false
		    }
		    
		},
		{
			"path": "pages/about/about",
			"style": {
				"navigationBarTitleText": "个人信息"
			}
		},
		{
			"path": "pages/cashDetails/cashDetails",
			"style": {
				"navigationBarTitleText": "交易明细"
			}
		}
    ],
	"tabBar": {
	    "list": [{
	      "pagePath": "pages/index/index",
	      "text": "首页",
		  "iconPath":"/static/未选中主页.png",
		  "selectedIconPath": "static/主页.png"
	    },{
		"pagePath": "pages/friends/friends",
		"text": "动态",
		"iconPath":"/static/dt2.png",
		"selectedIconPath": "static/dt.png"	
		},
		{
	      "pagePath": "pages/about/about",
	      "text": "个人信息",
		  "iconPath": "/static/我的 (2).png",
		  "selectedIconPath": "/static/我的 (4).png"
	    }]
	  },

	"globalStyle": {
		
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	    
	},
	"uniIdRouter": {}
	
		
		
}
